{
	"name": "vadage/oxidize",
	"type": "library",
	"description": "PHP adaptation of some rust concepts",
	"keywords": [
		"oxidize",
		"monad",
		"rust",
		"nullsafe",
		"exceptionless",
		"nullsafety"
	],
	"homepage": "https://github.com/vadage/oxidize",
	"license": "MIT",
	"require": {
		"php": ">=8.2"
	},
	"autoload": {
		"psr-4": {
			"Oxidize\\": "src"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "^11"
	},
	"autoload-dev": {
		"psr-4": {
			"Oxidize\\Test\\": "tests"
		}
	}
}
